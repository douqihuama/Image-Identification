    BEGIN
        MENUITEM "工具栏(&T)",                  ID_VIEW_TOOLBAR
        MENUITEM "状态栏(&S)",                  ID_VIEW_STATUS_BAR
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 hytImgPro(&A)...",       ID_APP_ABOUT
    END
    POPUP "图像处理(&P)"
    BEGIN
        MENUITEM "转化为灰度图(&R)\tCtrl+G",    ID_MENU_CONVERT_GRAY
        POPUP "灰度变换(&T)\tCtrl+T"
        BEGIN
            MENUITEM "反转变换",                    ID_MENU_OPPOSITE
            MENUITEM "灰度拉伸",                    ID_MENU_GRAY_STRECTHING
            MENUITEM "幂次变换",                    ID_MENU_POWER_TRANS
